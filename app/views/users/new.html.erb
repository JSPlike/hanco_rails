<section>
  <div class="section-wrap">
    <div class="section-form">
      <div style="text-align: center;">
        <span>회원가입</span>
      </div>
      <div class="signup-form">
        <%= form_tag(users_path) do %>
          <div class="space-co"></div>
          <div class="form-group has-success">
            <label class="control-label" for="inputSuccess1">Name</label>
            <input type="text" name="name" id="name" class="form-control" tabindex="0" placeholder="YOUR NAME....">
            </input>
          </div>
          <div class="space-co"></div>
          <div class="form-group has-success">
            <label class="control-label" for="inputSuccess1">Email</label>
            <input type="email" name="email" id="email" class="form-control" tabindex="0" placeholder="YOUR EMAIL....">
            </input>
          </div>
          <div class="space-co"></div>
          <div class="form-group has-success">
            <label class="control-label" for="inputSuccess1">Password</label>
            <input type="password" name="password" id="password" class="form-control" tabindex="0" placeholder="YOUR PASSWORD....">
            </input>
          <div class="space-co"></div>
          <div class="form-group has-success">
            <label class="control-label" for="inputSuccess1">Password Check</label>
            <input type="password" name="password_confirmation" id="password_confirmation" class="form-control" tabindex="0" placeholder="YOUR PASSWORD CHECK....">
            </input>
          <div class="space-co"></div>
          <div class="space-co"></div>
          <button type="submit" id="btnSignUp"class="btn btn-default btn-lg btn-block signuppage-btn-co">Create New Account</button>
        <%end%>
      </div>

      <div class="google-signup">
      </div>
    </div>
  </div>
</section>
<footer>
  <div class="section-wrap">
    <p style="color: gray">Handling Code <i>C</i> Copyright 2018 All rights   reserved</p>
  </div>
</footer>
<script type="text/javascript">
$(function() {
  $('#btnSignUp').click(function() {
      $.ajax({
          url: '/signup',
          data: $('form').serialize(),
          type: 'POST',
          success: function(response) {
              console.log(response);
          },
          error: function(error) {
              console.log(error);
          }
      });
  });
});
</script>